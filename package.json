{
  "name": "frontend",
  "version": "1.9.4",
  "private": true,
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=8.3.1"
  },
  "scripts": {
    "dev": "npm-run-all --parallel translate-watch develop:watch",
    "build": "npm run generate-tokens && next build && rm -rf ./dist/cache/",
    "build-ipfs": "npm run generate-tokens && next build && next export -o ipfsBuild && rm -rf ./dist && mkdir dist && mv ./ipfsBuild/* ./dist && rm -rf ./ipfsBuild",
    "start": "next start -p 8000",
    "start-ipfs": "NODE_ENV=production node ./server.js",
    "lint": "next lint",
    "translate-scan": "i18next-scanner 'src/**/*.{js,jsx,ts,tsx}'",
    "translate-watch": "npm run translate-scan && gulp watch",
    "develop:watch": "next dev -p 4001",
    "release": "npm version $npm_config_type --tag-version-prefix=\"release-\" --message=\"v%s\"",
    "style-dictionary": "node styled-components-tokens/style-dictionary.js",
    "make-tokens-files": "node styled-components-tokens/scripts/makeTokensFiles.js",
    "generate-tokens": "npm run make-tokens-files --project=tokens && npm run style-dictionary --project=tokens",
    "codegen": "graphql-codegen --config codegen.ts",
    "compile": "npm run generate && tsc",
    "type-check": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.17.0",
    "@ant-design/icons": "5.0.1",
    "@apollo/client": "3.7.10",
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/client-preset": "2.1.1",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typescript": "3.0.2",
    "@graphql-codegen/typescript-resolvers": "3.1.1",
    "@metamask/detect-provider": "1.2.0",
    "@rainbow-me/rainbowkit": "0.12.16",
    "@sentry/integrations": "7.38.0",
    "@sentry/nextjs": "7.38.0",
    "@sentry/tracing": "7.38.0",
    "@tanstack/react-query": "4.28.0",
    "@types/graphql": "14.5.0",
    "@types/lodash": "4.14.182",
    "@types/node": "18.14.4",
    "@types/nprogress": "0.2.0",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-world-flags": "1.4.2",
    "@types/redux-form": "8.3.5",
    "@types/redux-logger": "3.0.9",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.44.0",
    "acorn": "8.10.0",
    "address": "1.2.2",
    "antd": "5.10.1",
    "apollo": "2.34.0",
    "axios": "1.3.2",
    "dayjs": "1.11.7",
    "eslint-config-next": "13.2.3",
    "ethers": "5.7.2",
    "graphql": "14.7.0",
    "graphql-scalars": "1.20.4",
    "lodash": "4.17.21",
    "next": "13.2.3",
    "next-compose-plugins": "2.2.1",
    "next-export-i18n": "2.1.0",
    "next-images": "1.8.4",
    "next-translate-routes": "1.9.2",
    "nprogress": "0.2.0",
    "numbro": "2.3.6",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.8",
    "react-redux": "8.0.5",
    "react-world-flags": "1.5.1",
    "redux": "4.2.1",
    "redux-devtools-extension": "2.13.9",
    "redux-form": "8.3.10",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.4.2",
    "style-dictionary": "3.7.2",
    "styled-components": "5.3.8",
    "swiper": "9.2.4",
    "typescript": "4.9.5",
    "wagmi": "0.12.18",
    "web3": "1.7.4"
  },
  "devDependencies": {
    "@goodrequest/eslint-config-typescript-react": "1.2.0",
    "eslint": "8.35.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "gulp": "4.0.2",
    "i18next-scanner": "4.3.0",
    "lint-staged": "13.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint"
  }
}
